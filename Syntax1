a="goodmorning"
b="_"
print(a.capitalize())
print(a.upper())
print(a.lower())
print(a.title())
print(a.swapcase())
print(a.split())
print(a.center(20,"*"))
print(a.count("good"))
print(a.replace("morning","Night"))
print(b.join(a))
print(a.isupper())
print(a.islower())
print(a.isalpha())
print(a.isalnum())
print(a.isdigit())
print(a.isspace())
print(a.istitle())
print(a.startswith("g"))
print(a.endswith("i"))
print(a.find(" "))
print(len(a))
print(min(a))
print(max(a))
